<body class="u-body u-xl-mode" data-lang="es">
    <section class="u-align-center u-clearfix u-container-align-center u-palette-2-base u-section-3" id="sec-7af9">
        <div class="u-clearfix u-sheet u-valign-middle u-sheet-1">
            <div class="data-layout-selected u-clearfix u-expanded-width u-layout-wrap u-layout-wrap-1">
                <div class="u-layout">
                    <div class="u-layout-row">
                        <div class="u-container-align-left u-container-style u-layout-cell u-size-22 u-layout-cell-1" data-animation-name="customAnimationIn" data-animation-duration="1500">
                            <div class="u-container-layout u-container-layout-1">
                                <h3 class="u-align-left u-text u-text-default u-text-1">¡Edicion de Clientes!</h3>
                                <p class="u-align-left u-text u-text-default u-text-2"> Client Manager (Administrador de Clientes Edicion)</p>
                            </div>
                        </div>
                        <div class="u-container-align-center u-container-style u-layout-cell u-size-38 u-layout-cell-2" data-animation-name="customAnimationIn" data-animation-duration="1500" data-animation-delay="500">
                            <div class="u-container-layout u-valign-top u-container-layout-2">
                                <div class="u-form u-form-1">

                                    <table class="table">
                                        <tbody>
                                        <thead>

                                            <h4>Client</h4>
                                            <hr />
                                            <div class="row">
                                                <div class="col-md-4">
                                                    <form asp-action="Edit">
                                                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                                                        <div class="form-group">
                                                            <label asp-for="Name" class="control-label"></label> <br>
                                                            <input asp-for="Name" class="form-control" />
                                                            <span asp-validation-for="Name" class="text-danger"></span>
                                                        </div>
                                                        <div class="form-group">
                                                            <label asp-for="LastName" class="control-label"></label> <br>
                                                            <input asp-for="LastName" class="form-control" />
                                                            <span asp-validation-for="LastName" class="text-danger"></span>
                                                        </div>
                                                        <div class="form-group">
                                                            <label asp-for="Phone" class="control-label"></label> <br>
                                                            <input asp-for="Phone" class="form-control" />
                                                            <span asp-validation-for="Phone" class="text-danger"></span>
                                                        </div>
                                                        <div class="form-group">
                                                            <label asp-for="Email" class="control-label"></label> <br>
                                                            <input asp-for="Email" class="form-control" />
                                                            <span asp-validation-for="Email" class="text-danger"></span><br>
                                                        </div>
                                                        <input type="hidden" asp-for="Id" />
                                                        <div class="form-group">
                                                            <input type="submit" value="Save" class="u-btn u-btn-round u-btn-submit u-button-style u-palette-3-base u-radius-50 u-btn-2" />
                                                        </div>
                                                    </form>
                                                </div>
                                            </div>

                                            <div>
                                                <a asp-action="GetClient">Back to List</a>
                                            </div>
                                        </thead>


                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>


